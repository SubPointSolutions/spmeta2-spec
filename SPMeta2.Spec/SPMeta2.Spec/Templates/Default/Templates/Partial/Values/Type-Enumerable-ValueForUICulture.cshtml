@if (Model != null && Model.Count > 0)
{
    <table class="table table-bordered table-hover">

        <thead>
            <tr>
                <th>CultureName</th>
                <th>CultureId</th>
                <th>Value</th>

            </tr>
        </thead>

        <tbody>

            @foreach (var value in Model)
            {
                <tr>
                    <td>@value.CultureName</td>
                    <td>@value.CultureId</td>
                    <td>@value.Value</td>
                </tr>
            }
        </tbody>

    </table>
}
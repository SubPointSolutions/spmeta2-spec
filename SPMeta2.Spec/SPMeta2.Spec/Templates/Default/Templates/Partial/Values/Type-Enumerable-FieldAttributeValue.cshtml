@if (Model != null && Model.Count > 0)
{
    <table class="table table-bordered table-hover ">

        <thead>
            <tr>
                <th>Title</th>
                <th>Value</th>
            </tr>
        </thead>

        <tbody>

            @foreach (var value in Model)
            {
                <tr>
                    <td>@value.Title</td>
                    <td>@value.Value</td>
                </tr>
            }
        </tbody>

    </table>
}